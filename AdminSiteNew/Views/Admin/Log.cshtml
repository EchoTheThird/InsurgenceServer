@using System.Collections.Generic
@model List<AdminSiteNew.Models.AdminModels.AdminLogModel>

@{
    ViewBag.Title = "Admin Log";
    Layout = "/Views/Shared/_Layout.cshtml";
}

<table class="dataTable border bordered table-striped">
    <tr>
        <th>
            Id
        </th>
        <th>
            Moderator
        </th>
        <th>
            Type
        </th>
        <th>
            Data
        </th>
    </tr>
    @foreach (var l in Model)
    {
        <tr>
            <td>
                @l.Id
            </td>
            <td>
                @l.Moderator
            </td>
            <td>
                @l.Type.ToString()
            </td>
            <td>
                @l.Data
            </td>
        </tr>
    }
</table>
